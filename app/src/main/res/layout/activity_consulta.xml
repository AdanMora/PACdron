<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="pac.pac.pacdron.Consulta">

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="0dp"
        android:layout_height="103dp"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/paclogo"
        tools:layout_editor_absoluteX="0dp" />

    <EditText
        android:id="@+id/et_cedula"
        android:layout_width="275dp"
        android:layout_height="49dp"
        android:layout_marginTop="68dp"
        android:ems="10"
        android:inputType="number"
        app:layout_constraintTop_toBottomOf="@+id/imageView2"
        tools:layout_editor_absoluteX="55dp" />

    <Button
        android:id="@+id/btn_Consultar"
        android:layout_width="134dp"
        android:layout_height="46dp"
        android:layout_marginTop="60dp"
        android:text="Consultar"
        app:layout_constraintTop_toBottomOf="@+id/et_cedula"
        tools:layout_editor_absoluteX="125dp" />

    <TextView
        android:id="@+id/txtv_resultado"
        android:layout_width="278dp"
        android:layout_height="47dp"
        android:layout_marginTop="68dp"
        app:layout_constraintTop_toBottomOf="@+id/btn_Consultar"
        tools:layout_editor_absoluteX="53dp" />

</android.support.constraint.ConstraintLayout>
